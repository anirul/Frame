{
  "name": "frame",
  "version-string": "0.4.0-alpha",
  "homepage": "https://github.com/anirul/Frame/",
  "description": "Frame graphic library.",
  "dependencies": [
    "abseil",
    "glm",
    "glad",
    "gtest",
    "happly",
    {
      "name": "imgui",
      "features": [
        "opengl3-binding",
        "sdl3-binding",
        "vulkan-binding",
        "docking-experimental"
      ]
    },
    "opengl",
    "protobuf",
    {
      "name": "sdl3",
      "features": [ "vulkan" ]
    },
    "spdlog",
    "stb",
    "tinyobjloader",
    {
      "name": "vulkan"
    },
    "shaderc"
  ]
}
